<?xml version="1.0" encoding="utf-8"?>
<!-- This file contains a list of all the xml files that need to be parsed in this project. The number and name of these files don't matter as long as the Open Hardware Hub XML format is respected within those files. this means that the content of all these files can be combined into one file and it will still function properly. They are separated here for clarity. -->
<!-- Attributes are only used to specify paths or item orders between items with the same tag. -->
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.openhardwarehub.com/oProject_v0.1.xsd">
    <manifest>
    	<file url="bom.xml" />
    	<file url="files.xml" />
    	<file url="project.xml" />
    	<file url="steps.xml" />
    </manifest>
</project>
